-# We use slightly outdated libraries for making the photoswipe work.
%script{type: 'text/javascript', src: 'js/klass.min.js'}
-#%script{type: 'text/javascript', src: 'js/jquery-2.0.3.min.js'}
-#%script{type: 'text/javascript', src: 'js/jquery-1.6.4.min.js'}
-#%script{type: 'text/javascript', src: 'js/jquery-1.7.2.min.js'}
-#%script{type: 'text/javascript', src: 'js/jquery-1.9.1.min.js'}
%script{type: 'text/javascript', src: 'js/jquery-1.8.3.min.js'}
:javascript
  // Has to be Javascript for DD to be defined globally
  // Any init code here
  doInit = function() {
    console.log("Device Ready")
  };
  dd = $.Deferred();
  jqd = $.Deferred();
  $.when(dd, jqd).done(doInit);
  $(document).bind('mobileinit', function() {
    jqd.resolve()
  });
-#%script{type: 'text/javascript', src: 'js/jquery.mobile-1.3.2.js'}
%script{type: 'text/javascript', src: 'js/jquery.mobile-1.2.1.min.js'}  
%script{type: 'text/javascript', src: 'js/code.photoswipe.jquery-3.0.5.min.js'}  
%script{type: 'text/javascript', src: 'js/act_code.js'}    
%script{type: 'text/javascript', src: 'js/index.js'}
%script{type: 'text/javascript', src: 'phonegap.js'}
:javascript
  deviceReady = function() {
    dd.resolve();
  };
  document.addEventListener('deviceready', deviceReady, false);
  document.addEventListener('deviceready', function() {
    window.deviceReady();
  }, false);  
  // window.app.initialize();
%script{type: 'text/javascript', src: "http://js.pusher.com/2.1/pusher.min.js"}
